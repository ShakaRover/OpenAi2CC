{
  "name": "openai-to-claude-proxy",
  "version": "1.0.0",
  "description": "OpenAI API to Claude API Protocol Proxy",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "start:qwen": "ts-node src/index.ts --qwen-cli",
    "build:qwen": "npm run build && node dist/index.js --qwen-cli",
    "dev:custom": "ts-node src/index.ts --qwen-cli --qwen-oauth-file",
    "start:custom": "node dist/index.js --qwen-cli --qwen-oauth-file",
    "dev:openai": "ts-node src/index.ts --openai-api-key",
    "start:openai": "node dist/index.js --openai-api-key"
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.1"
  }
}